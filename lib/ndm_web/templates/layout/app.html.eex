<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <!-- Bootstrap Core CSS -->
        <link rel="stylesheet" href="<%= static_path(@conn, "/css/bootstrap.min.css") %>">
        <!-- MetisMenu CSS -->
        <link rel="stylesheet" href="<%= static_path(@conn, "/css/metisMenu.min.css") %>">
        <!-- Custom CSS -->
        <link rel="stylesheet" href="<%= static_path(@conn, "/css/startmin.css") %>">
        <!-- Custom Fonts -->
        <link rel="stylesheet" href="<%= static_path(@conn, "/css/font-awesome.min.css") %>">
        <!-- jQuery -->
        <script src="<%= static_path(@conn, "/js/jquery.min.js") %>"></script>
        <!-- Bootstrap Core JavaScript -->
        <script src="<%= static_path(@conn, "/js/bootstrap.min.js") %>"></script>
        <!-- Metis Menu Plugin JavaScript -->
        <script src="<%= static_path(@conn, "/js/metisMenu.min.js") %>"></script>
        <!-- Custom Theme JavaScript -->
        <script src="<%= static_path(@conn, "/js/startmin.js") %>"></script>
        <!-- Phoenix Sockets JavaScript -->
        <script src="<%= static_path(@conn, "/js/external/phoenix.js") %>"></script>
        <script src="<%= static_path(@conn, "/js/app.js") %>"></script>
    </head>
    <body>
        <main role="main">
                <%= render @view_module, @view_template, assigns %>
        </main>
    </body>
</html>